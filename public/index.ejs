<script>
    window.siteConfig = {
        name: "文件分享",
        footer: "Powered by Nginx AutoIndex & Skyone Beautify",
        readme: true,
        before: true
    }
</script>
<script>
    (function (){
        var color = localStorage.getItem("theme.palette.mode");
        if (color === "dark" || color === "light") {
            document.documentElement.style.colorScheme = color;
        }
        var styles = <%= JSON.stringify(htmlWebpackPlugin.files.css) %>
        for (var i in styles) {
            var style = document.createElement("link");
            style.rel = "stylesheet";
            style.href = styles[i];
            document.head.appendChild(style);
        }
    })()
</script><% for (const script of htmlWebpackPlugin.files.js ) { %>
<script defer="defer" src="<%= script %>"></script><% } %>